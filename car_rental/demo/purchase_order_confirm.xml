<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <function model="purchase.order" name="button_confirm">
        <value eval="[
        ref('purchase_order_1'),
        ref('purchase_order_2'),
        ref('purchase_order_3'),
        ref('purchase_order_4'),
        ref('purchase_order_5'),
        ref('purchase_order_6'),
        ref('purchase_order_7'),
        ref('purchase_order_8'),
        ref('purchase_order_9'),
        ref('purchase_order_10'),
        ref('purchase_order_11'),
        ref('purchase_order_12')]"

        />
    </function>

    <function name="button_validate" model="stock.picking">
        <value model="stock.picking" eval="(
            obj().env.ref('car_rental.purchase_order_1') +
            obj().env.ref('car_rental.purchase_order_2') +
            obj().env.ref('car_rental.purchase_order_3') +
            obj().env.ref('car_rental.purchase_order_4') +
            obj().env.ref('car_rental.purchase_order_5') +
            obj().env.ref('car_rental.purchase_order_7') +
            obj().env.ref('car_rental.purchase_order_8') +
            obj().env.ref('car_rental.purchase_order_9') +
            obj().env.ref('car_rental.purchase_order_11')
            ).picking_ids.ids"/>
    </function>
</odoo>
