<!--    analytic_line for task-->
    <record id="sale_order_2_account_analytic_line" model="account.analytic.line">
        <field name="employee_id" ref="hr.employee_admin"/>
        <field name="date" eval="(DateTime.now() + relativedelta(days=-13)).strftime('%Y-%m-%d')"/>
        <field name="unit_amount">201</field>
        <field name="x_out_odometer">3564.00</field>
        <field name="x_in_odometer">3765.00</field>
        <field name="project_id" model="project.project" eval="obj().env.ref('car_rental.sale_order_2').project_id.id"/>
        <field name="task_id" model="project.task" eval="obj().env.ref('car_rental.sale_order_2').tasks_ids.id"/>
    </record>
</odoo>
