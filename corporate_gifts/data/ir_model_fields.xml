<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <record id="field_x_color" model="ir.model.fields">
        <field name="name">x_color</field>
        <field name="model_id" ref="project.model_project_task"/>
        <field name="field_description">Number of Colors</field>
        <field name="ttype">integer</field>
    </record>
    <record id="field_x_logo_format" model="ir.model.fields">
        <field name="name">x_logo_format</field>
        <field name="field_description">Logo Format</field>
        <field name="model_id" ref="project.model_project_task"/>
        <field name="selection">[('PDF', 'PDF'), ('SVG', 'SVG'), ('Others', 'Others')]</field>
        <field name="ttype">selection</field>
    </record>
    <record id="field_x_print_size" model="ir.model.fields">
        <field name="name">x_print_size</field>
        <field name="field_description">Print Size</field>
        <field name="model_id" ref="project.model_project_task"/>
        <field name="selection">[('Pocket', 'Pocket'), ('Back', 'Back'), ('Front', 'Front'), ('Other', 'Other')]</field>
        <field name="ttype">selection</field>
    </record>
</odoo>
