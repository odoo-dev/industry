<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="appointment_type_1" model="appointment.type">
        <field name="name">Free e-Learn Skill Share</field>
        <field name="is_published" eval="True"/>
        <field name="lead_create" eval="True"/>
        <field name="reminder_ids" eval="[(6, 0, [ref('calendar.alarm_notif_1'), ref('calendar.alarm_mail_1'), ref('appointment_sms.calendar_alarm_data_1h_sms')])]"/>
        <field name="appointment_tz" model="res.users" eval="obj().env.ref('base.user_admin').tz or 'Europe/Brussels'"/>
        <field name="message_confirmation">
            <![CDATA[
                <p>Thanks you for trust. We will look forward with you.<br/></p>
            ]]>
        </field>
        <field name="message_intro">
            <![CDATA[
                <h1>Introduction to Skill Share</h1>
                <ul class="o_checklist">
                    <li id="checkId-1">Brief About Skill Share</li>
                    <li id="checkId-2">About Products</li>
                    <li id="checkId-3">Plans</li>
                    <li id="checkId-4">Policy</li>
                    <li id="checkId-5">Methodology</li>
                    <li id="checkId-6">Payment Process</li>
                </ul>
            ]]>
        </field>
    </record>
</odoo>
