<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="project_project_1" model="project.project">
        <field name="name">Internal</field>
        <field name="analytic_account_id" ref="account_analytic_account_1"/>
        <field name="stage_id" ref="project.project_project_stage_0"/>
        <field name="type_ids" eval="[(6, 0, [ref('hr_timesheet.internal_project_default_stage')])]"/>
        <field name="timesheet_product_id" eval="False"/>
    </record>
    <record id="project_project_4" model="project.project">
        <field name="name">Solar Installation Project</field>
        <field name="analytic_account_id" ref="account_analytic_account_4"/>
        <field name="stage_id" ref="project.project_project_stage_0"/>
        <field name="allow_worksheets" eval="True"/>
        <!-- <field name="type_ids" eval="[(6, 0, [ref('industry_fsm.planning_project_stage_0'), ref('industry_fsm.planning_project_stage_1'), ref('industry_fsm.planning_project_stage_2'), ref('industry_fsm.planning_project_stage_3'), ref('industry_fsm.planning_project_stage_4')])]"/> -->
        <field name="worksheet_template_id" ref="industry_fsm_report.fsm_worksheet_template"/>
        <field name="allow_billable" eval="True"/>
        <field name="allow_material" eval="True"/>
        <!-- <field name="is_fsm" eval="True"/> -->
        <field name="timesheet_product_id" ref="sale_timesheet.time_product"/>
    </record>

    <function model="project.project" name="write">
        <value
            eval="[
                ref('project_project_4'),
            ]" />
        <value eval="{'is_fsm': True}" />
    </function>
</odoo>
